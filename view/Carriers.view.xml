<?xml version="1.0" encoding="UTF-8" ?>
<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="com.bf.newtrial.view.Carriers"
    xmlns="sap.m">

    <Page
        title="Carriers">
        <List
            id="list"
            mode="{device>/listMode}"
            select="handleListSelect"
            items="{/Carriers}" >
            <StandardListItem
                icon="sap-icon://flight"
                type="{device>/listItemType}"
                press="handleItemSelect"
                title="{Carrname}"
                info="{Carrid}" />
        </List>
        <footer>
            <Bar />
        </footer>
    </Page>

</mvc:View>
